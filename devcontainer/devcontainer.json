"forwardPorts": [3000],
"portsAttributes": {
  "3000": {
    "label": "React App",
    "onAutoForward": "openPreview"
  }
},
"updateContentCommand": "npm install",
"postAttachCommand": "npm run dev",

// Configure tool-specific properties.
"customizations": {
  "codespaces": {
    "openFiles": ["src/App.tsx"]
  },
  "vscode": {
    "extensions": [
      "dbaeumer.vscode-eslint",
      "esbenp.prettier-vscode",
    ]
  },
